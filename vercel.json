{
  "version": 2,
  "name": "MicroServicesKubernetesTrailRun",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend/dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_ITEM_SERVICE_URL": "http://192.168.49.2:32002",
    "REACT_APP_USER_SERVICE_URL": "http://192.168.49.2:32001"
  }
}
